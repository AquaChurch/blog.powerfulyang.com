<style>
    .scale-footer-left .scale-footer-left-c{
        position:fixed;
        width:70px;
        height:70px;
        left: -5px;
        bottom: -4px;
        transition: 0.5s;
        z-index: 100;
    }
    .scale-footer-left:hover .scale-footer-left-d{
        position:fixed;
        width:70px;
        height:70px;
        left: 0;
        bottom: 0;
        transition: 0.5s;
        z-index: 100;
    }
    .scale-footer-left:hover .scale-footer-left-c{
        display: none;
    }
    .scale-footer-left .scale-footer-left-d{
        display: none;
    }
    .scale-footer-left:hover .scale-footer-left-d{
        display: block;
        transform: scale(1.1);
    }
    .scale-footer-left-sansejin{
      position: fixed;
      width: 200px;
      right: 200px;
      bottom: 0;
      z-index: 8;
    }
    @media (max-width: 768px) {
        .scale-footer-left-sansejin{
            display: none;
        }
    }
    .lower{
      bottom:0;
    }
</style>
<script>
    function remove(e) {
        e.parentNode.removeChild(e);
    }
</script>
<div class="mdl-mini-footer--left-section sns-list">
